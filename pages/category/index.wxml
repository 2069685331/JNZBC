<!--pages/category/index.wxml-->
<!--轮播图-->
<view class="index_swiper">
  <swiper autoplay="{{true}}" indicator-dots="{{true}}" circular="{{true}}">   
    <swiper-item wx:for="{{swiperList}}"
    wx:key="goode_id">
      <navigator class="" target="" url="" hover-class="navigator-hover" open-type="navigate">
        <image mode="widthFix" src="{{item.image_src}}">
        </image>
      </navigator>
    </swiper-item>
  </swiper>
</view>
  <!--轮播图-->

  <!--分区-->
<view class="index_cates">
    <navigator wx:for="{{catesList}}" wx:key="name" url="" hover-class="navigator-hover" open-type="navigate">
        <image mode="widthFix" class="" src="{{item.image_src}}"  lazy-load="false" binderror="" bindload="">
            
        </image>
    </navigator>
</view>
<!--分区-->
